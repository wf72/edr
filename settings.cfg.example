[Main]
export_ip_file = True
Verbose = True
log_write = True
zb_file = True
max_url_check = 50
# по каким словам или ключам определять страницу блокировки
find_words = ["Доступ закрыт", "Доступ ограничен", "Доступ запрещён"]
# Время ожидания ответа при проверке блокировки
check_timeout = 5


[URLS]
